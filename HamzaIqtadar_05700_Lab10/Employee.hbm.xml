<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="Employee" table="fileinfo2">
      <meta attribute="class-description">
         This class contains the employee detail. 
      </meta>
      <id name="id" type="int" column="id">
         <generator class="native"/>
      </id>
      <property name="filename" column="first_name" type="string"/>
      <property name="path" column="path" type="string"/>
      <property name="folder" column="folder" type="string"/>
      <property name="hash" column="hash" type="string"/>
   </class>
</hibernate-mapping>